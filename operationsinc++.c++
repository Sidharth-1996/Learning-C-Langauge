using namespace std;
#include<iostream>
int main(){
     int a,b;
    cout<<"Enter the number :\n";
    cin>>a;
    cout<<"Enter the second number :\n";
    cin>>b;
    cout<<"The value of a+b is : "<<a+b<<"\n";
    cout<<"The value of a-b is : "<<a-b<<"\n";
    cout<<"The value of a*b is : "<<a*b<<"\n";
    cout<<"The value of a/b is : "<<a/b<<"\n";
}